CREATE TABLE ACCTYPE (
	ACCTYPE_ID INT(11) NOT NULL AUTO INCREMENT,
	ACCTYPE_NAME VARCHAR(25) NOT NULL,
	ACCTYPE_DESC VARCHAR(MAX) NOT NULL,
	ACCTYPE_STAT BIT(1) NOT NULL DEFAULT 0,
	PRIMARY KEY (ACCTYPEID)
)

CREATE TABLE ACC (
	ACCID INT(11) NOT NULL AUTO INCREMENT,
	ACCTYPE_FK INT(11) NOT NULL,
	ACCUSERNAME VARCHAR(25) NOT NULL,
	ACCPASSWORD VARCHAR(25) NOT NULL,
	PRIMARY KEY (ACCID),
	FOREIGN KEY (ACCTYPE_FK) REFERENCES EEMS_ACCTYPE (ACCTYPE_ID),
	CREATED_AT TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	ACC_STAT BIT(1) NOT NULL DEFAULT 0

)

CREATE TABLE PROGRAM
(
	PROG_ID INT(11) NOT NULL AUTO INCREMENT,
	PROG_NAME VARCHAR(MAX) NOT NULL,
	PROG_INIT VARCHAR(10) NOT NULL,
	PRIMARY KEY (PROG_ID),
	PROG_STAT BIT(1) NOT NULL DEFAULT 0

)

CREATE TABLE ACADYR (
	ACADYR INT(11) NOT NULL AUTO INCREMENT,
	ACADYR_NAME VARCHAR(50) NOT NULL,
	ACADYR_STAT BIT(1) NOT NULL DEFAULT 0
)

CREATE TABLE BUILDING (
	BLDG_ID INT(11) NOT NULL AUTO INCREMENT,
	BLDG_NAME VARCHAR(MAX) NOT NULL,
	BLDG_STAT BIT(1) NOT NULL DEFAULT 0
)

CREATE TABLE ROOM (
	ROOM_ID INT(11) NOT NULL AUTO INCREMENT,
	ROOM_NAME VARCHAR(50) NOT NULL,
	ROOM_STAT BIT(1) NOT NULL DEFAULT 0
)

CREATE TABLE SCHED (
	SCHED_ID INT(11) NOT NULL AUTO INCREMENT,
	ACADYR_FK INT(11) NOT NULL,
	PROG_FK INT(11) NOT NULL,
	SCHED_TIME VARCHAR(MAX) NOT NULL,
	SCHED_DATE DATE NOT NULL,
	SCHED_STAT BIT(1) NOT NULL DEFAULT 0	

)

CREATE TABLE SCHED_DETAILS (
	SCHEDDTL_ID INT(11) NOT NULL AUTO INCREMENT,
	SCHED_FK INT(11) NOT NULL,
	PRIMARY KEY (SCHEDDTL_ID),
	FOREIGN KEY (SCHED_FK) REFERENCES SCHED(SCHED_ID),


)

CREATE TABLE APPLICANT (
	APP_ID INT(11) NOT NULL AUTO INCREMENT,
	PRIMARY KEY (APP_ID),
	APP_FNAME VARCHAR(50) NOT NULL,
	APP_LNAME VARCHAR(50) NOT NULL,
	APP_MNAME VARCHAR(50),
	APP_SEX BIT(1) NOT NULL,
	APP_BDATE DATE NOT NULL,
	APP_PBIRTH VARCHAR(MAX) NOT NULL,
	APP_CIVIL VARCHAR(25) NOT NULL,
	APP_CITIZENSHIP VARCHAR(25) NOT NULL,
	APP_DISABILITY VARCHAR(MAX) NOT NULL DEFAULT "NONE",
	APP_PIC MEDIUMBLOB,
	APP_CONTACT VARCHAR(25) NOT NULL,
	APP_EMAIL VARCHAR(50) NOT NULL,
	APP_FIRST_FK INT(11) NOT NULL,
	APP_SECOND_FK INT(11),
	APP_THIRD_FK INT(11),
	FOREIGN KEY (APP)

	)